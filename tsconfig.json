{
    "compilerOptions": {
      "target": "es5",
      "module": "esnext",
      "allowJs": true,
      "jsx": "react",
      "sourceMap": true,
      "outDir": "./dist",
      "noEmit": true,
      "isolatedModules": true,
      "strict": false,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "noFallthroughCasesInSwitch": true,
      "resolveJsonModule": true
    },
    "exclude": [
      "node_modules",
      "**/*.spec.ts"
    ],
    "include": [
      "./src/**/*.tsx"
, "src/main-weather-display/world-map/map-of-usa.tsxx", "src/enum/enums.ts"    ]
  }